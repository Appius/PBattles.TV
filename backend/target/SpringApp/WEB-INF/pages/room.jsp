        $(document).ready(function() {
            /*Resize chat height up to maximum*/
            var documentHeight = $('html').height() - $('header').height() - $('footer').height();//200 - indents
            $('.panel-body').css('height',documentHeight-17);
            $('.panel-border').css('height',documentHeight);

            /*Popover for smile button*/
            $('[data-toggle="popover"]').popover();
            $('[data-toggle="tooltip"]').tooltip({
                trigger: 'click'
            }).on("click", function() {
                setTimeout(function(){
                    $('[data-toggle="tooltip"]').tooltip('hide');
                },500);
            });
        });

        $('#apply-settings').click(function () {
            var btn = $(this);
            btn.button('loading');
            setTimeout(function(){
                btn.button('reset')
                $('#settings').modal('hide');
            },1000);
        });

        /*It's for dropdown as 'select' list*/
        $("#btn-language").find("ul").on('click', 'li a', function(){
            $('#btn-language').find('button:first-child [role="text"]').text($(this).text());
        });
        $("#prefered-area").find("ul").on('click', 'li a', function(){
            $('#prefered-area').find('button:first-child [role="text"]').text($(this).text());
        });
    </script>
